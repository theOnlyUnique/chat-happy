{
  "package": "com.example.demo",
  "name": "demo2",
  "versionName": "1.0.0",
  "versionCode": 1,
  "icon": "/assets/images/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    {
      "name": "system.fetch"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "appType": "quickapp",
  "template/official": "demo-template",
  "config": {
    "logLevel": "debug"
  },
  "router": {
    "entry": "pages/Demo",
    "pages": {
      "pages/Demo": {
        "component": "index"
      },
      "pages/DemoDetail": {
        "component": "index"
      }
    }
  },
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "pages": {
      "pages/Demo": {
        "titleBarText": "应用示例模版"
      },
      "pages/DemoDetail": {
        "titleBarText": "详情页"
      }
    }
  }
}
